{
	"extends": ["eslint:recommended", "plugin:vue/recommended"],
	"plugins": ["vue"],
	"env": {
		"browser": true,
		"node": true
	},
	"globals": {
	},
	"parserOptions": {
		"sourceType": "module",
		"ecmaVersion": 12
	},
	"rules": {
		"no-console": 0,
		"no-mixed-spaces-and-tabs": 0,
		"no-unexpected-multiline": 2,
		"linebreak-style": ["error", "unix"]
	}
}
