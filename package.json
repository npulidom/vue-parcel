{
	"name": "vue-parcel",
	"description": "Simple boilerplate for building static SPA's with VueJs 2.",
	"license": "UNLICENSED",
	"main": "app/app.js",
	"repository": "https://github.com/npulidom/vue-parcel",
	"scripts": {
		"watch": "rm -rf dist && parcel app/index.pug --dist-dir dist --cache-dir .parcel --no-source-maps",
		"build": "rm -rf dist && parcel build app/index.pug --dist-dir dist --cache-dir .parcel --no-source-maps --detailed-report 20",
		"clean": "rm -rf dist .parcel"
	},
	"browserslist": "> 0.5%, last 2 versions, not dead",
	"devDependencies": {
		"@parcel/config-default": "~2.0.1",
		"@parcel/core": "~2.0.1",
		"@parcel/optimizer-cssnano": "~2.0.1",
		"@parcel/transformer-pug": "~2.0.1",
		"@parcel/transformer-stylus": "~2.0.1",
		"@parcel/transformer-vue": "~2.0.1",
		"@vue/component-compiler-utils": "~3.3.0",
		"autoprefixer": "~10.4.0",
		"cssnano": "~5.0.11",
		"parcel-config-vue2": "~0.1.3",
		"parcel-optimizer-css-url": "~2.0.1",
		"parcel-transformer-vue2": "~0.1.7",
		"postcss": "~8.3.11",
		"pug": "~3.0.2",
		"stylus": "~0.55.0",
		"vue-hot-reload-api": "~2.3.4",
		"vue-template-compiler": "~2.6.14"
	},
	"dependencies": {
		"modern-normalize": "~1.1.0",
		"vue": "~2.6.14",
		"vue-router": "~3.5.3",
		"vuex": "~3.6.2"
	}
}
